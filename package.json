{
  "name": "vue-notion",
  "version": "1.1.1",
  "description": "A Vue renderer for Notion pages",
  "repository": "github:janniks/vue-notion",
  "main": "dist/esm.js",
  "browser": "dist/esm.js",
  "module": "dist/esm.js",
  "unpkg": "dist/min.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.css",
    "src/**/*.css",
    "nuxt/*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "example": "nuxt example",
    "example:pack": "npm pack && cd example && npm install && npm install ../*.tgz && npm run build && npm run generate",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "release:minor": "npm version minor -m 'Release %s' && clear && npm publish",
    "release:patch": "npm version patch -m 'Release %s' && clear && npm publish",
    "postpublish": "./scripts/postpublish.sh",
    "prepare": "rm -rf dist && npm run build"
  },
  "dependencies": {
    "cross-fetch": "^3.0.6",
    "defu": "^3.2.2",
    "vue-fragment": "^1.5.1",
    "vue-prism-component": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "prismjs": "^1.22.0",
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  }
}
